{
  "extends": ["ultracite/core", "ultracite/react"],
  "javascript": {
    "formatter": {
      "quoteStyle": "single",
      "jsxQuoteStyle": "single",
      "semicolons": "asNeeded"
    }
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noUnknownAtRules": {
          "options": {
            "ignore": ["@tailwind"]
          }
        }
      }
    }
  }
}
